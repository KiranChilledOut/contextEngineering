{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false
      }
    },
    "logLevel": {
      "default": "debug",
      "Host": "debug", 
      "Function": "debug"
    },
    "console": {
      "isEnabled": true
    }
  },
  "functionTimeout": "00:10:00",
  "http": {
    "routePrefix": "api"
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}